#include "Math.decah"
class U {
	float p = 0;
	Math m = new Math();
	void u(float f, float r){
		float approch =  m.asin(f);
		if ((approch - r)*(approch - r) > this.p*this.p) {
		this.p = approch - r;
		}
		print(f, ": ");
		print(approch - r);
		if (approch==r){print(" ok");}
		println();

	}
}
