class B{
	int getx() { return 1;}
	int gety() { return 0;}
	boolean equals(Object o){
	A a = (A)(o);
	return a.getx() == getx();}
}

class A extends B{
	int getx() { return 1;}
	int gety() { return 1;}
	boolean equals(Object o){
	A a = (A)(o);
	return a.gety() == gety();}
}

